@page
@model Contoso.Pages.AboutModel

@{
    ViewData["Title"] = "Student Body Statistics";
}


<h1>Student Body Statistics.</h1>
<table class="table">
    <thead>
        <tr>
            <th>
                Enrollment Date
            </th>
            <th>
                Students
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Student)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.EnrollmentData)
            </td>
            <td>
                @item.StudentCount
            </td>
           <td></td>
        </tr>
        }
    </tbody>
</table>

